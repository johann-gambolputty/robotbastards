
//
//	This effect file is used when rendering to a depth texture
//


technique DefaultTechnique
{
	pass
	{
		LightingEnable	= false;
		FrontFace		= CW;
		DepthTestEnable	= true;
		DepthMask		= true;
		CullFaceEnable	= true;
		PolygonOffset	= float2( 0, 0 );
		PolygonOffsetFillEnable	= false;
	}
}
